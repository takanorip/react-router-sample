{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "classes": true,
      "jsx": true
   }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react",
    "class-property"
  ],
  "rules": {
    "react/display-name": 1,
    "class-property/rule-name": 2
  }
}
